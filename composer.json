{
    "name": "aligent/orocommerce-invoices-bundle",
    "description": "OroCommerce Bundle to allow Customers to View/Pay Invoices via Storefront Portal",
    "type": "library",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Aligent\\InvoiceBundle\\": "./src/Aligent/InvoiceBundle"
        },
        "exclude-from-classmap": [
            "./src/Aligent/InvoiceBundle/Tests/"
        ]
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "oro/commerce": "5.0.*",
        "aligent/oro-fixtures": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8",
        "phpmd/phpmd": "^2.12",
        "friendsofphp/php-cs-fixer": "~2.18.2 || ~2.19.0",
        "symfony/phpunit-bridge": "~4.4.24 || ~6.1.0 || ~6.2.0",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
